<!-- include header template -->
<% include partials/header %>

<h1>The Post Page!</h1>

<% for(var i=0; i< array.length; i++){ %>
<li>
    <%=array[i].title%> -
    <%=array[i].author%>
</li>
<%} %>

<!-- Same thing, but looks better -->
<% array.forEach(function(post){ %>
<li>
    <%=post.title%> -
    <strong>
        <%=post.author%> </strong>
</li>
<% }) %>

<!-- include footer template -->
<% include partials/footer %>